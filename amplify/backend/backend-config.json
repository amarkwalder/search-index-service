{
	"hosting": {
		"S3AndCloudFront": {
			"service": "S3AndCloudFront",
			"providerPlugin": "awscloudformation"
		}
	},
	"auth": {
		"SearchIndexServiceAuth": {
			"service": "Cognito",
			"providerPlugin": "awscloudformation",
			"dependsOn": []
		}
	},
	"api": {
		"SearchIndexServiceAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "AzureADTokenHandler",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"function": {
		"MSOneNoteReader": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"AzureADTokenHandler": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "auth",
					"resourceName": "SearchIndexServiceAuth",
					"attributes": [
						"UserPoolId"
					]
				}
			]
		},
		"UpdateSearchIndex": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		},
		"MSOneNoteWriter": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true
		}
	}
}