{
    "hosting": {
        "S3AndCloudFront": {
            "service": "S3AndCloudFront",
            "providerPlugin": "awscloudformation"
        }
    },
    "auth": {
        "SearchIndexServiceAuth": {
            "service": "Cognito",
            "providerPlugin": "awscloudformation",
            "dependsOn": []
        }
    },
    "api": {
        "SearchIndexServiceAPI": {
            "service": "API Gateway",
            "providerPlugin": "awscloudformation",
            "dependsOn": [
                {
                    "category": "function",
                    "resourceName": "AzureADTokenHandler",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "MSOneNoteReader",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "ElasticSearchIndex",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "MSOneNoteWriter",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                },
                {
                    "category": "function",
                    "resourceName": "Query",
                    "attributes": [
                        "Name",
                        "Arn"
                    ]
                }
            ]
        }
    },
    "function": {
        "MSOneNoteReader": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "AzureADTokenHandler": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true,
            "dependsOn": [
                {
                    "category": "auth",
                    "resourceName": "SearchIndexServiceAuth",
                    "attributes": [
                        "UserPoolId"
                    ]
                }
            ]
        },
        "MSOneNoteWriter": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "ElasticSearchIndex": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        },
        "Query": {
            "service": "Lambda",
            "providerPlugin": "awscloudformation",
            "build": true
        }
    }
}